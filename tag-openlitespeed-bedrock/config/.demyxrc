DISABLE_AUTO_UPDATE="true"
DISABLE_UPDATE_PROMPT="true"
EDITOR="nano"
POWERLEVEL9K_DISABLE_CONFIGURATION_WIZARD=true
TERM="xterm-256color"
ZSH_AUTOSUGGEST_HIGHLIGHT_STYLE="fg=#444444"
ZSH_THEME="powerlevel10k/powerlevel10k"
plugins=(git zsh-autosuggestions autoupdate)
source "$DEMYX_CONFIG"/.p10k.zsh
export GPG_TTY="$TTY"
export NVM_DIR="$HOME/.nvm"
# This loads nvm
[ -s "$NVM_DIR/nvm.sh" ] && source "$NVM_DIR/nvm.sh"
# This loads nvm bash_completion
[ -s "$NVM_DIR/bash_completion" ] && source "$NVM_DIR/bash_completion"

# Echo out demyx-sage commands
echo -e "\n\e[33m[INFO]\e[39m To install sage theme: demyx-sage new [theme name]"
echo -e "\e[33m[INFO]\e[39m To start browsersync after sage theme is installed: demyx-sage cmd [theme name] start"
echo -e "\e[33m[INFO]\e[39m For more info: demyx-sage help\n"
